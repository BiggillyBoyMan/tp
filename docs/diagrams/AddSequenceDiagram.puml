@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":BizBookParser" as BizBookParser LOGIC_COLOR
participant ":AddCommandParser" as AddCommandParser LOGIC_COLOR
participant "a:AddCommand" as AddCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("add n/Google i/Technology a/SWE Intern\nt/Backend e/recruit@google.com s/Saved d/2024-12-31")
activate LogicManager

LogicManager -> BizBookParser : parseCommand("add n/Google...")
activate BizBookParser

create AddCommandParser
BizBookParser -> AddCommandParser
activate AddCommandParser

AddCommandParser --> BizBookParser
deactivate AddCommandParser

BizBookParser -> AddCommandParser : parse("n/Google i/Technology a/SWE Intern\nt/Backend e/recruit@google.com s/Saved d/2024-12-31")
activate AddCommandParser

create AddCommand
AddCommandParser -> AddCommand
activate AddCommand

AddCommand --> AddCommandParser : returns AddCommand
deactivate AddCommand

AddCommandParser --> BizBookParser : a
deactivate AddCommandParser
AddCommandParser -[hidden]-> BizBookParser
destroy AddCommandParser

BizBookParser --> LogicManager : a
deactivate BizBookParser

LogicManager -> AddCommand : execute(m)
activate AddCommand

AddCommand -> Model : hasInternshipApplication(application)
activate Model

Model --> AddCommand : false
deactivate Model

AddCommand -> Model : addInternshipApplication(application)
activate Model

Model --> AddCommand
deactivate Model

create CommandResult
AddCommand -> CommandResult
activate CommandResult

CommandResult --> AddCommand
deactivate CommandResult

AddCommand --> LogicManager : r
deactivate AddCommand

[<--LogicManager
deactivate LogicManager
@enduml

