@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Model as ModelPackage <<Rectangle>>{
Class "<<interface>>\nReadOnlyBizBook" as ReadOnlyBizBook
Class "<<interface>>\nReadOnlyUserPrefs" as ReadOnlyUserPrefs
Class "<<interface>>\nModel" as Model
Class BizBook
Class ModelManager
Class UserPrefs

Class UniqueCompanyList
Class InternshipApplication
Class CompanyName
Class JobType
Class Email
Class Description
Class ApplicationStatus
Class Deadline
Class Industry

Class I #FFFFFF
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

BizBook .up.|> ReadOnlyBizBook

ModelManager .up.|> Model
Model .right.> ReadOnlyUserPrefs
Model .left.> ReadOnlyBizBook
ModelManager -left-> "1" BizBook
ModelManager -right-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs

BizBook *--> "1" UniqueCompanyList
UniqueCompanyList --> "~* all" InternshipApplication
InternshipApplication *--> CompanyName
InternshipApplication *--> JobType
InternshipApplication *--> Email
InternshipApplication *--> Description
InternshipApplication *--> ApplicationStatus
InternshipApplication *--> Deadline
InternshipApplication *--> Industry

InternshipApplication -[hidden]up--> I
UniqueCompanyList -[hidden]right-> I

CompanyName -[hidden]right-> JobType
JobType -[hidden]right-> Email
Email -[hidden]right-> Description
Description -[hidden]right-> ApplicationStatus
ApplicationStatus -[hidden]right-> Deadline
Deadline -[hidden]right-> Industry

ModelManager --> "~* filtered" InternshipApplication
@enduml
